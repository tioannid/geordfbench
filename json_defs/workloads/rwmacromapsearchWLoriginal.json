      "avgFunc" : "QUERYSET_MEAN",
      "onColdFailure" : "EXCLUDE_AND_PROCEED",
      "clearCacheDelaymSecs" : 5000
    },
    "toponymTemplateName" : "<TOPONYME>",
    "rectangleTemplateName" : "<RECTANGLE_LITERAL>"
